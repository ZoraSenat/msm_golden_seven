<h1>Add Director</h1>

<form action="/directors/create">
<label for="dob">DOB</label>
<input type="text" name="dob" id="dob" placeholder="DOB"></input>

<label for="name">Name</label>
<input type="text" name="name" id="name" placeholder="Name"></input>

<label for="bio">Bio</label>
<input type="text" name="bio" id="bio" placeholder="Bio"></input>

<label for="image_url">Image URL</label>
<input type="text" name="image_url" id="image_url" placeholder="Image URL"></input>

<button type="submit">Create Director</button>

</form>
